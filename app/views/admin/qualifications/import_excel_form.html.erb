<!-- app/views/admin/qualification/import_excel.html.erb -->
<%= form_for @qualification, url: import_excel_admin_qualifications_path, method: :post, html: { multipart: true } do |f| %>
 <div class="field">
  <%= label_tag :excel_file, 'Excel File' %>
  <%= f.file_field :excel_file, accept: '.xlsx' %>
 </div>
 <%= f.submit 'Import Excel' %>
<% end %>