<!-- app/views/admin/questions/import_csv.html.erb -->
<%= form_for @question, url: import_csv_admin_questions_path, method: :post, html: { multipart: true } do |f| %>
 <div class="field">
  <%= label_tag :csv_file, 'CSV File' %>
  <%= f.file_field :csv_file, accept: '.csv' %>
 </div>
 <%= f.submit 'Import CSV' %>
<% end %>